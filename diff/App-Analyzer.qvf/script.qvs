        End If

        Let vNextURL = '&next=' & '$(vNextURL)';

        Let vNextURLRows = NoOfRows('NextURL');

        Drop Table NextURL;
        
        Let vCounter = $(vCounter) + 1;
        
    Loop While $(vNextURLRows)>0;
    
    Inner Join (LargeAppConsumption)
